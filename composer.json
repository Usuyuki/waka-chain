{
  "name": "usuyuki/waka-chain",
  "description": "A library for creating temporary password-like by waka.",
  "type": "library",
  "license": "MIT",
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Usuyuki\\WakaChain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Usuyuki\\WakaChain\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "bamarni-bin": {
      "target-directory": "tools"
    }
  },
  "authors": [
    {
      "name": "Usuyuki",
      "email": "usuyuki524@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "bamarni/composer-bin-plugin": "^1.5"
  },
  "require": {
    "php": "^8.1",
    "php-di/php-di": "^6.4"
  },
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
